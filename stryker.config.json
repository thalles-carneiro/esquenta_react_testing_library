{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "mutate": [
    "src/components/CharCard.js",
    "src/components/Header.js",
    "src/components/Navbar.js",
    "src/pages/Characters.js",
    "src/pages/Home.js",
    "src/pages/NotFound.js",
    "src/routes/index.js",
    "src/App.js"
  ],
  "mutator": {
    "name": "javascript",
    "excludedMutations": [
      "ArithmeticOperator",
      "ArrayDeclaration",
      "ArrowFunction",
      "BlockStatement",
      "BooleanLiteral",
      "ConditionalExpression",
      "EqualityOperator",
      "LogicalOperator",
      "ObjectLiteral",
      "UnaryOperator",
      "UpdateOperator"
    ]
  },
  "testRunner": "jest",
  "reporters": [
    "progress",
    "clear-text",
    "html"
  ],
  "coverageAnalysis": "off",
  "jest": {
    "projectType": "create-react-app"
  }
}